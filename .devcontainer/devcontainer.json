{
 "name": "Python & PostgreSQL Dev Container",
 "dockerComposeFile": "docker-compose.yml",
 "service": "app",
 "workspaceFolder": "/workspace",
 "forwardPorts": [5432],
	"portsAttributes": {
		"5432": {"label": "PostgreSQL port", "onAutoForward": "silent"}
	},
 "customizations": {
    "vscode": {
        "extensions": 
        [
            "ms-python.python",
            "ms-python.vscode-pylance",
            "charliermarsh.ruff"
        ]
    }
 }
}